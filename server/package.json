{
  "name": "server",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --ext ts,js,json --exec yarn start",
    "build": "yarn install",
    "start": "ts-node src/server.ts",
    "test": "jest --collectCoverage",
    "verify": "yarn tsc && yarn test"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.11",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "ts-node": "^10.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.{jsx,tsx,ts,js}",
      "!dist/**",
      "!babel.config.js",
      "!questions.ts"
    ]
  }
}
